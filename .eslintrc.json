{
  "root": true,
  "env": {
    "jest": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "jest",
    "prettier",
    "simple-import-sort"
  ],
  "rules": {
    "simple-import-sort/sort": "error"
  }
}
